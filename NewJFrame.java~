    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Equates Simple intrest when pressed
        double principle = 0;
        double time = 0;
        double rate = 0;
        double amount = 0;
        
        principle = Double.parseDouble(jTextField4.getText()); // gets varibles.
        rate = Double.parseDouble(jTextField5.getText());
        time = Double.parseDouble(jTextField6.getText());
        
        amount = (principle * rate * time) + principle; // calcs. simple intrest
        
        if (principle < 0 || time < 0 || rate < 0) // safe guard errors in entering
            {
              amount = 0;
            }
        
        jTextField7.setText(String.format("%.3f", amount)); //tells amount    
    }                                        

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // button to equate compund intrest.
        
        double principle = 0;
        double time = 0;
        double rate = 0;
        double amount = 0;
        double compTime = 0;
        
        principle = Double.parseDouble(jTextField3.getText()); // gets varibles.
        rate = Double.parseDouble(jTextField8.getText());
        time = Double.parseDouble(jTextField10.getText());
        compTime = Double.parseDouble(jTextField9.getText());
        
        amount = Math.pow((1+(rate/compTime)),(time*compTime)); //calcs. compound intrest
        
        amount = amount * principle;
        
         if (principle < 0 || time < 0 || rate < 0 || compTime < 0) //no errors in entering
            {
              amount = 0;
            }
         jTextField11.setText(String.format("%.3f", amount)); //outputs amount
         
    }                                        

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // resets the boxes that contain currency to be converted to empty.
        jTextField1.setText(null); 
        jLabel9.setText(null);
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Button to convert Money
        
        double CAD = Double.parseDouble(jTextField1.getText()); // Gets cash
        
        if (jComboBox1.getSelectedItem().equals("USD")) // if combobox equals currency for all
        {
            jLabel9.setText(String.format("USD $%.2f", CAD*0.83)); // converts currecny for all
        }
        
        else if (jComboBox1.getSelectedItem().equals("GBP"))
        {
            jLabel9.setText(String.format("GBP $%.2f", CAD*0.59));
        }
        
        else if (jComboBox1.getSelectedItem().equals("JPN"))
        {
            jLabel9.setText(String.format("JPN $%.2f", CAD*90.33));
        }
        
        if (jComboBox1.getSelectedItem().equals("BRL"))
        {
            jLabel9.setText(String.format("BRL $%.2f", CAD*4.35));
        }
        
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Button to reset all simple intrest boxes
       
        jTextField4.setText(null);
        jTextField5.setText(null);
        jTextField6.setText(null);
        jTextField7.setText(null);
    }                                        

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Button to reset all coumpund intrest boxes
        
        jTextField3.setText(null);
        jTextField8.setText(null);
        jTextField9.setText(null);
        jTextField10.setText(null);
        jTextField11.setText(null);
        
    }                                        

    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Button to output random stocks
        Random random = new Random();
        int rand = random.nextInt(8); // gets random num
        
        String [] stocks = {"$TSLA", "$AMZN", "$GME", "$TSLA", "$AAPL", "$MSFT", "$FB", "$GOOGL"};
        jLabel11.setText(stocks[rand]); // outputs random num from array
    }                                        

    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Button to output random pin number
        Random random = new Random();
        int rand = random.nextInt(8); // gets random num
        int[] pin = new int[4]; //makes pin array
        
        for (int i = 0; i < 4; i++)
        {
          int num = random.nextInt(10);  //enters random num into pun array
          pin[i] = num;
        }
        jLabel13.setText(String.format("" + pin[0])); //prints array in boxes.
        jLabel14.setText(String.format("" + pin[1]));
        jLabel15.setText(String.format("" + pin[2]));
        jLabel16.setText(String.format("" + pin[3]));
      
    }                                        

